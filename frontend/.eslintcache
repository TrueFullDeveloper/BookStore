tchLogin\",\r\n  async userData => {\r\n    try {\r\n      const res = await axios.post(\r\n        \"https://jsonplaceholder.typicode.com/posts\",\r\n        JSON.stringify(userData)\r\n      );\r\n\r\n      localStorage.setItem(\r\n        storageName,\r\n        JSON.stringify({\r\n          userId: 10, // TODO: Here Should Be res.userId\r\n        })\r\n      );\r\n\r\n      return 10; // TODO: Here Should Be res.userId\r\n    } catch (e) {\r\n      console.log(e.message);\r\n    }\r\n  }\r\n);\r\n\r\nexport const fetchSignup = createAsyncThunk(\r\n  \"auth/fetchSignup\",\r\n  async userData => {\r\n    try {\r\n      const res = await axios.post(\r\n        \"https://jsonplaceholder.typicode.com/posts\",\r\n        JSON.stringify(userData)\r\n      );\r\n\r\n      localStorage.setItem(\r\n        storageName,\r\n        JSON.stringify({\r\n          userId: 10, // TODO: Here Should Be res.userId\r\n        })\r\n      );\r\n\r\n      return 10; // TODO: Here Should Be res.userId\r\n    } catch (e) {\r\n      console.log(e.message);\r\n    }\r\n  }\r\n);\r\n\r\nconst authSlice = createSlice({\r\n  name: \"auth\",\r\n  initialState: {\r\n    userId: null,\r\n    isAuthenticated: false,\r\n    loading: false,\r\n  },\r\n\r\n  reducers: {\r\n    logout(state) {\r\n      state.userId = null;\r\n      state.isAuthenticated = false;\r\n      state.loading = false;\r\n    },\r\n\r\n    login(state, { payload: userId }) {\r\n      state.userId = userId;\r\n      state.isAuthenticated = true;\r\n      state.loading = false;\r\n    },\r\n  },\r\n\r\n  extraReducers: {\r\n    [fetchLogin.pending]: state => {\r\n      state.loading = true;\r\n    },\r\n\r\n    [fetchLogin.fulfilled]: (state, { payload: userId }) => {\r\n      state.userId = userId;\r\n      state.isAuthenticated = true;\r\n      state.loading = false;\r\n    },\r\n\r\n    [fetchSignup.pending]: state => {\r\n      state.loading = true;\r\n    },\r\n\r\n    [fetchSignup.fulfilled]: (state, { payload: userId 